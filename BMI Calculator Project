    "def cal():\n",
    "     (\"BMI Calculator formula = Weight(kg)/Height(m) **2\"),\n",
    "     the_height = float(input(\"Enter your Height in centimeters\"))\n",
    "     print(the_height),\n",
    "     the_weight = float(input(\"Enter your Weight in kg\"))\n",
    "     print(the_weight),\n",
    "     the_BMI = the_weight/(the_height/100)**2\n",
    "     print(\"Your Body Mass Index is\",the_BMI)\n",
    "     if(the_BMI<= 18.5):\n",
    "        print(\"You are severely Underweight\")\n",
    "        print(\"You should consult nutritionist and take a right diet plan for your Health\")\n",
    "     elif(the_BMI<= 24.9):\n",
    "        print(\"You are Normal\")\n",
    "        print(\"Congrats!you are Healthy\")\n",
    "     elif(the_BMI<= 29.9):\n",
    "        print(\"OOPS!You are Over weight\"),\n",
    "        print(\"You should do regular exercise and maintain a right diet plan\")\n",
    "     elif(the_BMI<= 34.9):\n",
    "        print(\"You are severely Over weight\")\n",
    "     elif (the_BMI<= 39.9):\n",
    "        print(\"You are obesity\")\n",
    "     else:\n",
    "         print(\"enter valid details\")\n",
    "   "
